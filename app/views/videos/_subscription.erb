<div id="zype_<%= video['_id'] %>"></div>
<script src="https://player.zype.com/embed/<%= video['_id'] %>.js?autoplay=true&access_token=<%= session[:access_token] %>" type="text/javascript"> </script>
<% if !user_signed_in? %>
  <div id="paywall">
    <%= render partial: 'sessions/login_form', locals: { is_paywall: true, form_id: 'paywall-login-form' } %>
  </div>
<% end %>

<script type="text/javascript">
  $(document).ready(function(){
    $('#paywall-login-form').modal('show');
  });
</script>
